<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest</title>
    <!-- @include 'styles.kit' -->

</head>

<body>
    <!-- @include 'nav.kit' -->
    <section class="contact" id="contact">
        <div class="wrapper">
            <h2 class="contact-title">Skontaktuj się z nami</h2>
            <form class="contact__form">
                <div class="contact__form-box">
                    <label for="name">Imię:</label>
                    <input type="text" id="name">
                </div>
                <div class="contact__form-box">
                    <label for="mail">E-mail:</label>
                    <input type="email" id="mail">
                </div>
                <div class="contact__form-box">
                    <label for="msg">Wiadomość:</label>
                    <textarea id="msg" id="" cols="30" rows="10"></textarea>
                </div>
                <button type="submit" class="contact__form-btn">Wyślij</button>
            </form>
        </div>
    </section>

    <section class="map">
        <div class="wrapper">
            <h2 class="map-title">Jak do nas dojechać?</h2>
            <iframe class="googlemap"
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.846417481827!2d19.892409015718332!3d50.089162279427086!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a32728962db%3A0x4344067c5fa03761!2sJasnog%C3%B3rska%202%2C%2031-358%20Krak%C3%B3w!5e0!3m2!1spl!2spl!4v1646303870011!5m2!1spl!2spl"
                width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
        </div>
    </section>
    <!-- @include 'footer.kit' -->

</body>

</html>